unit dijkstra;

interface
  uses
    graphLib, Messages;
  const
    CLUSTER=25;
  type
    Tplength=array of longword;
    TpathTo=array of array of longword;
    TDijkstra=record
      pathLength=Tplength;
      pathTo=TpathTo;
    end;
  function calcDijkstra(graph:graphNet; d:graphDot):TDijkstra;
implementation

end.
